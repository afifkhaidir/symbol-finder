{
  "name": "symbol-finder",
  "description": "Easily browse and insert symbols in Sketch",
  "version": "0.0.2",
  "engines": {
    "sketch": ">=3.0"
  },
  "skpm": {
    "name": "Symbol Finder",
    "manifest": "src/manifest.json",
    "main": "symbol-finder.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.4",
    "@skpm/extract-loader": "^2.0.2",
    "css-loader": "^3.2.0",
    "html-loader": "^0.5.5"
  },
  "resources": [
    "resources/**/*.js"
  ],
  "dependencies": {
    "sketch-module-web-view": "^3.1.4"
  },
  "author": "afifkhaidir <afif.khaidir@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/afifkhaidir/symbol-finder.git"
  }
}
